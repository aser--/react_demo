{
  "extends": "airbnb",
  "rules": {
    "comma-dangle": [1, "never"],
    "spaced-comment": [0, "always"],
    "func-names": 0
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "plugins":[
    "react",
    "jsx-a11y",
    "import"
  ]
}